<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean name="articleIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_ARTICLE"/>
	</bean>
	
	<bean name="reportIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_REPORT_MST"/>
	</bean>
	
	<bean name="proposalIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_PROPOSAL_DETAIL"/>
	</bean>

	<bean name="circleIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_CIRCLE_MST"/>
	</bean>
	
	<bean name="circleDtlIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_CIRCLE_DETAIL"/>
	</bean>
	
	<bean name="kpiUserIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="defaultDataSource" />
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="TB_IDS"/>
		<property name="tableName"	value="TB_KPI_USER_DETAIL"/>
	</bean>
	
	
</beans>
