/* main */

.sub_visual_wrap {position:relative; overflow:hidden; width:100%; height:485px; background: url(/def/img/main/img_sub_visual1.jpg) no-repeat 50% 0;}

/* 콘텐츠 */
/* con_header */
#con_header {position:relative; width:100%; height:140px; line-height:140px; overflow:hidden; border-bottom:1px solid #d6d6d6; text-align:center;}

#con_header h3{color:#27499d; font-size:32px; font-weight:bold; letter-spacing:-1px; }
#con_header .text{color:#27499d; font-size:20px; font-weight:bold; position:absolute; top:0; right:0; opacity:0.5;}

/* con_body */
#con_body {width:100%; min-height:500px; margin:0 0 20px 0; overflow:hidden;}

.location_box{position:relative; width:100%; height:65px; line-height:65px; background:#666464; }
.location_box .location {position:relative; width:1130px;  margin:0 auto;   z-index:1;}
.location_box .location a {float:left; padding:0 25px; color:#333; text-decoration:none;  display:inline-block; height: 65px;}
.location_box .location a.position {color:#222; font-weight:600;}
.location_box .location div{position:relative; font-size:13px; float:left;border-right: 1px solid #4d4b4b;}
.location_box .location div a{width:100%; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;font-size: 20px;color: #fff;font-weight: 600;padding-right: 110px;} 
.location_box .location div a img {position:absolute; top:28px; right:18px;  float:right;}
.location_box .location a.home img {width:16px; margin: 22px 0 0 0;}
.location_box .location div.first{border-left:1px solid #4d4b4b;}
.location_box .location ul{position:absolute; top:65px; width:100%; background:#fff; border-right:1px solid #dedede; border-bottom:1px solid #dedede; max-height:408px; overflow-y:auto;border-left: 1px solid #dedede;}
.location_box .location ul li{width:100%; border-bottom:1px solid #dedede; overflow:hidden;}
.location_box .location ul li:last-child{border-bottom:0;}
.location_box .location ul li a{ width:100%; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;color: #666;padding: 0 25px;}
.location_box .location a.print {float:right; border-right:1px solid #dedede;}
.location_box .location a.print img {vertical-align:text-bottom;}
.location_box .location span{display:none;}