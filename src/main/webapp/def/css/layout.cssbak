@charset "utf-8";
/* freecommerce (http://www.freedi.kr/) WSU 201905 */
/* 폰트 */
@font-face {
    font-family:'nanum';
    src:url('/def/font/nanum.eot');
    src:url('/def/font/nanum.eot?#iefix') format('embedded-opentype'), url('/def/font/nanum.woff') format('woff'), url('/def/font/nanum.ttf') format('truetype');
    font-weight:normal;
    font-style:normal; 
}

/* skipnavigation */
#skipnavigation {position:relative; z-index:99999;}
#skipnavigation ul li {position:absolute;  width:100%; text-align:center;}
#skipnavigation ul li a {display:block; height:1px; width:1px; margin-bottom:-1px; padding:0; overflow:hidden; text-align:center; color:#000; white-space:nowrap;}
#skipnavigation ul li a:hover, #skipnavigation ul li a:focus, #skipnavigation ul li a:active { display:block; height:auto; width:100%; padding:10px 0; margin:0; font-size:15px; font-weight:bold; color:#fff; background:#0b4db1; text-align:center; }

#wrap {position:relative; width:100%;min-width: 1130px;}

/* header */
#header_wrap {position:absolute; width:100%; min-width:1030px; left:0; top:0; background:url(/def/img/common/bg_header_wrap.png) repeat-x top;  z-index:3; *zoom:1;  }
#header_wrap .bg_header_wrap {width:100%; min-width:1130px; min-height:125px;}
#header {position:relative; width:1130px; margin:0 auto; z-index:4;}

#header:after {content:""; display:block; clear:both;}

	#lnb {position:absolute;  top: 0;right: 15px;}
		#lnb ul {overflow:hidden;}
		#lnb ul.lnb_mn li {padding:0 15px 0 0 ; margin:0 15px 0 0; float:left;  text-decoration:none; font-size:14px; }
		#lnb ul.lnb_mn li img{vertical-align:middle;}
		#lnb ul.lnb_mn li:last-child{background:none; padding:0; margin:0;}
		#lnb ul.lnb_mn li a{color:#585a69;}
		#lnb ul.lnb_mn li a:hover,
		#lnb ul.lnb_mn li a:focus{text-decoration:none;}

	#header .header_top {position:relative; width:1130px;  margin-top:25px;}
	#header .header_top h1 {position:absolute;  left:0; }
	#header .header_top h1 a{display:block;}
	#header .header_top h1 img{width:260px;}

	/* 주메뉴 */
	#gnb_mn {width:1130px; overflow:hidden; z-index:99;}
	#gnb_mn ul{overflow:hidden; padding:0 40px 0 20px;width: 880px;margin: 35px 0 0 250px;}
	#gnb_mn ul li{float:left; width:20%; text-align:center;}
	#gnb_mn ul li a{font-size:20px; height:40px; line-height:40px; margin-top:5px; color:#000000; font-weight:bold; display:inline-block; }
	#gnb_mn ul li a:hover,
	#gnb_mn ul li a:focus{text-decoration:none; border-top:5px solid #27499d; margin-top:0; margin-bottom:5px;}
	#gnb_mn ul li:last-child a{background:none;}
	#gnb_mn ul li div {width:100%; height:260px; margin:80px 0 0 0;  display:none;}
	#gnb_mn ul li:last-child div {border-right:none;}
	#gnb_mn ul li div ul {padding:0; margin:0;}
	#gnb_mn ul li div ul li {float:none; width:100%; padding-left:40px; text-align:left; }
  #gnb_mn ul li div.top3 ul li{padding-left:32px;}
  #gnb_mn ul li div ul li a {display:inline-block; font-size:18px; height:36px; line-height:36px; font-weight:bold; background:none; color:#000; margin-top:0; margin-bottom:0;}
	#gnb_mn ul li div ul li a:hover,
  #gnb_mn ul li div ul li a:hover {text-decoration:underline; border-top:0; margin-bottom:0;}
	#gnb_mn .gnb_img {position:absolute; top:160px; left:0;}
	
#container_wrap {position:relative; width:100%;  background:#fff;}
#container_box {position:relative; width:1130px; padding:40px 0 65px 0; margin:0 auto; overflow:hidden;}
.grey_wrap {background:#f3f3f3;}
#main_container_box {position:relative; width:1130px; padding:0; margin:0 auto; overflow:hidden;}

/* footer */
#footer_wrap {display:inline-block; width:100%; height:180px;  z-index:2; border-top: 1px solid #ddd;padding-top: 40px;}
	#footer {position:relative; width:1130px; height:95px; margin:0 auto; color:#8c8c8b; line-height: 95px;font-weight: bold;text-align: center;}
	#footer img{margin-right:20px;}

/* index */
#container_wrap .index_cont_box {position:relative; width:100%; overflow:hidden; }

/* 비주얼영역 */
#index_visual_wrap{position:relative; width:100%; height:700px; overflow:hidden;}
#index_visual {position:absolute; top:0; left:0; width:100%; min-width:1130px; height:700px; z-index:1; }
#index_visual #bg div {position:absolute; left:0; top:0; width:100%; height:700px; z-index:0; display:block; text-align:left; font-size:18px; line-height:28px; color:#eceff0;}
#bgChanger {position:absolute; left:0px; top:650px; width:100%; height:30px;  z-index:10; text-align:center;}
#bgChanger ul {display:inline-block;  vertical-align:top;}
#bgChanger ul li {display:inline-block; padding:7px 0; vertical-align:top;}
#bgChanger ul li a {display:block; font-size:0; line-height:0; width:42px; height:3px; background: #62575d; margin:0 2px;}
#bgChanger ul li a.current {background:#070707;}
#bgChanger div.control {display:inline-block; padding:0 0 0 2px; }
#bgChanger div.control a {position:relative; display:block; padding:3px; line-height:0; font-size:0; z-index:1;}
#bgChanger div.control a:before {position:absolute; display:block; left:0; top:0; content:""; width:11px; height:13px;  z-index:-1;}

